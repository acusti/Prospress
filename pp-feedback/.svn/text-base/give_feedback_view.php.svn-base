<div class="wrap give-feedback" id="give-feedback">
	<?php if( isset ( $_POST [ 'feedback_submit' ] ) ):?>
		<div id="message" class="updated fade">
			<p><strong>
				Feedback Submitted!
			</strong></p>
		</div>
	<?php endif; ?>
	<?php if ( isset( $errors ) && is_wp_error( $errors ) ) : ?>
		<div class="error">
			<ul>
			<?php foreach( $errors->get_error_messages() as $message )
				echo "<li>$message</li>";
			?>
			</ul>
		</div>
	<?php endif; ?>
	<h2><?php _e('Give Feedback'); ?></h2>
	<form name="GiveFeedback_Form" id="GiveFeedback_Form" action="<?php echo $_SERVER ['REQUEST_URI']; ?>" method="post">
		<table width='100%' cellspacing="0" cellpadding="0">
			<tr id="GiveFeedback_Comment">
				<td class="comment_label">
					<label><?php _e( 'Comment' ) ?></label>
				</td>
				<td class="comment_input">
					<input type="text" name="feedback_comment" id="feedback_comment" class="input" value="<?php echo $_POST['feedback_comment']; ?>" 
					<?php echo $disabled; ?> 
					/>
				</td>
			</tr>
			<tr id="GiveFeedback_Rating">
				<td class="rating_label">
					<label><?php _e( 'Rating' ) ?></label>
				</td>
				<td class="rating_input">
					<input name="feedback_score" type="radio" id="rating_positive" value='2' 
					<?php echo $disabled; ?> 
					<?php echo (isset($_POST['feedback_score']) && $_POST['feedback_score'] == 2 ) ? 'checked="checked"' : ''; ?> 
					/> <?php _e('Positive'); ?>
					<input name="feedback_score" type="radio" id="rating_neutral" value='1' 
					<?php echo $disabled; ?> 
					<?php echo (isset($_POST['feedback_score']) && $_POST['feedback_score'] == 1 ) ? 'checked="checked"' : ''; ?> 
					/> <?php _e('Neutral'); ?>
					<input name="feedback_score" type="radio" id="rating_negative" value='0' 
					<?php echo $disabled; ?> 
					<?php echo (isset($_POST['feedback_score']) && $_POST['feedback_score'] == 0 ) ? 'checked="checked"' : ''; ?> 
					/> <?php _e('Negative'); ?>
				</td>
			</tr>
			<tr id="GiveFeedback_Submit">
				<td id="GiveFeedback_SubmitButton">
					<input type="submit" name="feedback_submit" id="feedback_submit" class="button-secondary action" value="<?php _e('Submit Feedback'); ?>" tabindex="100"  
					<?php echo $disabled; ?> 
					/>
					<input type="hidden" name="redirect_to" value="http://<?php echo $_SERVER['SERVER_NAME'] . $_SERVER['REQUEST_URI'] ?>" />
					<input type="hidden" name="for_user_id" value="<?php echo $for_user ?>" />
					<input type="hidden" name="from_user_id" value="<?php echo $from_user ?>" />
					<input type="hidden" name="marketplace_id" value="<?php echo $marketplace_id ?>" />
					<input type="hidden" name="listing_id" value="<?php echo $listing_id ?>" />
					<input type="hidden" name="role" value="<?php echo $role ?>" />
					<input type="hidden" name="feedback_date" value="<?php echo current_time('mysql', true); ?>" />
				</td>
			</tr>
		</table>
	</form>
</div>